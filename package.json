{
  "name": "tafystudio",
  "version": "0.0.0",
  "private": true,
  "description": "Robot Distributed Operation System (RDOS) for accessible robotics",
  "packageManager": "pnpm@9.3.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:unit": "turbo test:unit",
    "test:integration": "turbo test:integration",
    "test:e2e": "turbo test:e2e",
    "format": "turbo format",
    "format:check": "turbo format:check",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules",
    "validate": "./scripts/validate.sh",
    "validate:quick": "make pre-push-quick",
    "validate:full": "make pre-push-full",
    "pre-push": "make pre-push",
    "docs:build": "turbo docs:build",
    "docs:validate": "turbo docs:validate"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typedoc": "^0.28.10",
    "typedoc-plugin-markdown": "^4.8.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}